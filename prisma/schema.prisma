generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("LOCAL_DATABASE_URL")
}

model User {
  id         Int      @id @default(autoincrement())
  email      String   @unique
  nickname   String?  @unique
  password   String?
  avatar     String?
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
